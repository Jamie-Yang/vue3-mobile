{
  "name": "vue3-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --mode dev",
    "build": "npm run type-check && vite build",
    "build:test": "npm run type-check && vite build --mode test",
    "build:pre": "npm run type-check && vite build --mode pre",
    "build:prod": "npm run type-check && vite build --mode prod",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "run-p lint:prettier lint:eslint lint:stylelint",
    "lint:prettier": "prettier . --write --loglevel warn",
    "lint:eslint": "eslint . --ext .vue,.js,.cjs,.mjs,.ts --fix --ignore-path .gitignore",
    "lint:stylelint": "stylelint \"./src/**/*.{vue,scss,css}\" --fix",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "pinia": "^2.1.6",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.5.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.16.8",
    "@types/qs": "^6.9.8",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-html": "^1.5.0",
    "prettier": "3.0.3",
    "sass": "^1.69.0",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-scss": "^5.2.1",
    "terser": "^5.21.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.18"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.16.0",
    "npm": ">=9.0.0"
  }
}
